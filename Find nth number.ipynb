   "source": [
    "number = int(input('Enter number'))\n",
    "c = 0\n",
    "p = 0\n",
    "l=[]\n",
    "while p!=number+1:\n",
    "    c += 1\n",
    "    for i in range(2,c):\n",
    "        if c%i == 0:\n",
    "            break\n",
    "    else:\n",
    "        l.append(c)\n",
    "        p+=1\n",
    "print(max(l))"
   ]
